{% extends 'base.html.twig' %}

{% block body %}


<div class="example-wrapper">
    <h3>Ajouter un nouveau questionaire </h3>

    <p>
        Veuiller remplir  les champs avec les infromations du patient
    </p>

    <form method="post" action="/survey/ajouterAction">
        <div class="form-group">
            <label>Titre</label>
            <input type="text" class="form-control" id="Titre" name="Titre" aria-describedby="Titre" placeholder="Titre">
        </div>
        <div class="form-group">
            <label>Lien</label>
            <input type="text" class="form-control" id="Lien" name="Lien" aria-describedby="Lien" placeholder="Lien">
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>
    </form>


</div>
{% endblock %}
